                map: map
            });
           
            var marker = new google.maps.Marker({
                position: { lat: 43.846506, lng: 18.354426 },
                map: map
            });
           
            var marker = new google.maps.Marker({
                position: { lat: 44.534124, lng: 18.652804 },
                map: map
            });


            var marker = new google.maps.Marker({
                position: { lat: 43.348512, lng: 17.804460 },
                map: map
            });
            
            var marker = new google.maps.Marker({
                position: { lat: 44.819229, lng: 15.852279 },
                map: map
            });

      

            var marker = new google.maps.Marker({
                position: { lat: 44.198228, lng: 17.911572 },
