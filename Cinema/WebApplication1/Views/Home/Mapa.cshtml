                zoom: 13
            });
       
            
            var marker = new google.maps.Marker({
                position: { lat: 43.855910, lng: 18.418361 },
                map: map
            });

            
            var marker = new google.maps.Marker({
                position: { lat: 43.858601, lng: 18.41594 },
                map: map
            });
           
            var marker = new google.maps.Marker({
